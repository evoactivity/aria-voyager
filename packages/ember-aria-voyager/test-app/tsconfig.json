{
  "extends": "@tsconfig/ember",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "ember-aria-voyager-test-app/tests/*": [
        "tests/*"
      ],
      "ember-aria-voyager-test-app/*": [
        "app/*"
      ]
    }
  },
  "include": [
    "app/**/*",
    "tests/**/*",
    "types/**/*"
  ],
  "glint": {
    "environment": [
      "ember-loose",
      "ember-template-imports"
    ]
  }
}
